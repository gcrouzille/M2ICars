@model IEnumerable<M2ICarsASP.Models.Reservation>
@using M2ICarsASP.Models
    {
    ViewBag.Title = "search";
}

<section class="container search-container">
    <h2>Search</h2>
    <div class="map-section">
        <div class="col-md-3">
       
                    <h4>Liste des Clients</h4>                 
                          

            @foreach (Reservation r in Model)

            {
                <div class="funkyradio-success">
                     
                        <input type="radio" name="driverRadio" id="@r.ClientId" value="@r.ClientId.ToString()"checked />
                   
                       

                    <label for="@r.ClientId">@r.ClientId</label>
                </div>
            }

                        <div class="form-group">
                            <div class="col-md-10">
                                <input id="search" type="submit" value="valider" class="btn btn-default" />
                            </div>
                        </div>
                    </div>

                 
        </div>
        <div id="map" class="col-md-9">

        </div>
    
    <img src="~/Content/Images/search.jpg" />
</section>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCDjKjiTE-YwsyVbG2KY4VVJF5w3F7XWt8&libraries=places&callback=initAutocomplete"
        async defer></script>
<script src="~/Scripts/placesAutocompletion.js"></script>

